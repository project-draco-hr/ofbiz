{
  if (condition == null) {
    condition=current;
  }
 else {
    List condList=UtilMisc.toList(condition,current);
    if (this.isOrSet) {
      condition=EntityCondition.makeCondition(condList,EntityOperator.OR);
    }
 else {
      condition=EntityCondition.makeCondition(condList,EntityOperator.AND);
    }
  }
  this.setNot(false);
}
