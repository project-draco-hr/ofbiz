{
  if (wasMouseClicked()) {
    String text="";
    try {
      text=m_edit.getText();
    }
 catch (    NullPointerException e) {
      text="";
    }
 finally {
      if (text.length() > 1) {
        m_edit.setText(text.substring(0,text.length() - 1));
      }
 else {
        m_edit.setText("");
      }
    }
    m_dialog.repaint();
    return;
  }
}
