{
  Object obj;
  try {
    obj=ObjectType.getInstance(clazz);
  }
 catch (  Exception e) {
    Debug.logError(e,module);
    return false;
  }
  if (obj != null && (obj instanceof ServiceSecurity)) {
    ServiceSecurity sec=(ServiceSecurity)obj;
    return sec.hasPermission(dctx,context);
  }
 else {
    return false;
  }
}
