{
  if (this.comError > 2) {
    return;
  }
  try {
    this.openCalled=true;
    ((jpos.CashDrawer)control).openDrawer();
    this.openCalled=false;
  }
 catch (  JposException e) {
    Debug.logError(e,module);
    this.comError++;
    PosScreen.currentScreen.showDialog("dialog/error/drawererror",this);
  }
}
