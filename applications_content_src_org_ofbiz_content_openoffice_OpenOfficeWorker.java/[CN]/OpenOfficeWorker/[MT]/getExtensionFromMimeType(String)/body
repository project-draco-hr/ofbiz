{
  String extension="";
  if (UtilValidate.isEmpty(mimeType)) {
    extension="html";
  }
 else   if (mimeType.equalsIgnoreCase("application/pdf")) {
    extension="pdf";
  }
 else   if (mimeType.equalsIgnoreCase("application/msword")) {
    extension="doc";
  }
 else   if (mimeType.equalsIgnoreCase("text/html")) {
    extension="html";
  }
 else {
    extension="html";
  }
  return extension;
}
