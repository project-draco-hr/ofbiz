{
  selectGroup.appendTo(sb);
  if (unioned != null) {
    unioned.appendTo(sb);
  }
  if (relations != null) {
    StringUtil.appendTo(sb,relations.values()," ",null,null);
  }
  if (orderBy != null) {
    sb.append(" ORDER BY ");
    StringUtil.append(sb,orderBy,null,null,", ");
  }
  if (offset != -1) {
    sb.append(" OFFSET ").append(offset);
  }
  if (limit != -1) {
    sb.append(" LIMIT ").append(limit);
  }
  sb.append(';');
  return sb;
}
