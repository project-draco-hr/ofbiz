{
  String directory=null;
  if (filePath.indexOf("/") != -1) {
    int lastIndexOf=filePath.lastIndexOf("/");
    directory=filePath.substring(0,(lastIndexOf + 1));
  }
 else   if (filePath.indexOf("\\") != -1) {
    int lastIndexOf=filePath.lastIndexOf("\\");
    directory=filePath.substring(0,(lastIndexOf + 1));
  }
  return directory;
}
