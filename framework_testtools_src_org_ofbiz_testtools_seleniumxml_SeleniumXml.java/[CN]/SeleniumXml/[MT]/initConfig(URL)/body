{
  try {
    if (url == null) {
      String errMsg="The Java environment (-Dxxx=yyy) variable with name " + url.toString() + " is not set, cannot resolve location.";
      throw new MalformedURLException(errMsg);
    }
    props=UtilProperties.getProperties(url);
  }
 catch (  IOException e) {
    e.printStackTrace();
    throw e;
  }
}
