{
  StringBuilder sb=new StringBuilder("<entity-data ");
  sb.append("location=\"").append(this.locationFse).append("\" ");
  sb.append("mode=\"").append(this.mode).append("\" ");
  sb.append("timeout=\"").append(this.timeout).append("\" ");
