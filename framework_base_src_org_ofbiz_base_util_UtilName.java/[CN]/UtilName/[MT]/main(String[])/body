{
  StringBuilder name=new StringBuilder();
  for (  String arg : args) {
    if (name.length() != 0) {
      name.append(" ");
    }
    name.append(arg);
  }
  Map<String,String> nameMap=parseName(name.toString(),true);
  for (  Map.Entry<String,String> entry : nameMap.entrySet()) {
    System.out.println(entry.getKey() + " - " + entry.getValue());
  }
}
