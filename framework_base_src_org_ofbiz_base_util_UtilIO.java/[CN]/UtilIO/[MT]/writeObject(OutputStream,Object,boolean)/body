{
  try {
    PrintWriter writer=new PrintWriter(new OutputStreamWriter(out,UTF8));
    if (encodeObject(writer,value,allowJsonResolve)) {
      writer.println();
      writer.close();
      return;
    }
  }
 catch (  Exception e) {
  }
  ObjectOutputStream oout=new ObjectOutputStream(out);
  oout.writeObject(value);
  oout.close();
  out.close();
}
