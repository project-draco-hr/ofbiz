{
  if (flds.size() < 1) {
    return sb;
  }
  sb.append(prefix);
  Iterator<ModelField> fldsIt=flds.iterator();
  while (fldsIt.hasNext()) {
    ModelField field=fldsIt.next();
    sb.append(field.colName);
    if (fldsIt.hasNext()) {
      sb.append(separator);
    }
  }
  sb.append(afterLast);
  return sb;
}
