{
  StringBuffer returnString=new StringBuffer();
  if (flds.size() < 1) {
    return "";
  }
  int i=0;
  for (; i < flds.size() - 1; i++) {
    returnString.append(((ModelField)flds.get(i)).colName);
    returnString.append(" like {");
    returnString.append(i);
    returnString.append("} AND ");
  }
  returnString.append(((ModelField)flds.get(i)).colName);
  returnString.append(" like {");
  returnString.append(i);
  returnString.append("}");
  return returnString.toString();
}
