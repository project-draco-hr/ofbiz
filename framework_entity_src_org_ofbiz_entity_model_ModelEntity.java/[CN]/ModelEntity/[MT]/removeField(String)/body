{
  if (fieldName == null)   return null;
  ModelField field=null;
  for (int i=0; i < fields.size(); i++) {
    field=fields.get(i);
    if (field.name.equals(fieldName)) {
      fields.remove(i);
      if (field.isPk) {
        pks.remove(field);
      }
 else {
        nopks.remove(field);
      }
    }
    field=null;
  }
  return field;
}
