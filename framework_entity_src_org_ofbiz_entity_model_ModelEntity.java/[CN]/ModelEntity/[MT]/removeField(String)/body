{
  if (fieldName == null)   return null;
synchronized (fieldsLock) {
    ModelField field=fieldsMap.remove(fieldName);
    if (field != null) {
      this.fieldsList.remove(field);
      if (field.getIsPk()) {
        pks.remove(field);
      }
 else {
        nopks.remove(field);
      }
    }
    return field;
  }
}
