{
  if (this.hasFieldWithAuditLog == null) {
    this.hasFieldWithAuditLog=false;
    for (    ModelField mf : this.fields) {
      if (mf.getEnableAuditLog()) {
        this.hasFieldWithAuditLog=true;
      }
    }
    return this.hasFieldWithAuditLog;
  }
 else {
    return this.hasFieldWithAuditLog;
  }
}
