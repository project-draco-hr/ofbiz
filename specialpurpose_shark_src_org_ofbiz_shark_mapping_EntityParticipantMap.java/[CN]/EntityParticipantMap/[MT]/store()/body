{
  if (newValue) {
    try {
      delegator.create(participant);
      newValue=false;
    }
 catch (    GenericEntityException e) {
      throw new RootException(e);
    }
  }
 else {
    try {
      delegator.store(participant);
    }
 catch (    GenericEntityException e) {
      throw new RootException(e);
    }
  }
}
