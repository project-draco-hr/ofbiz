{
  if (config != null) {
    DocumentFragment fragment=barcodeUtil.generateSVGBarcode(config,message);
    try {
      String barcodeXml=UtilXml.writeXmlDocument(fragment);
      return barcodeXml;
    }
 catch (    IOException e) {
      Debug.logError(e,"Error turning the SVG barcode fragment into a String",module);
      return "";
    }
  }
 else {
    return "";
  }
}
