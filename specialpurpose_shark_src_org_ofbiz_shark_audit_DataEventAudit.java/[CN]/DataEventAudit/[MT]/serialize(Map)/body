{
  ByteArrayOutputStream bos=null;
  ObjectOutputStream oos=null;
  byte[] bytes=null;
  try {
    bos=new ByteArrayOutputStream();
    oos=new ObjectOutputStream(bos);
    oos.writeObject(map);
    oos.flush();
    bytes=bos.toByteArray();
  }
 catch (  IOException e) {
    Debug.logError(e,module);
  }
 finally {
    if (oos != null) {
      try {
        oos.close();
      }
 catch (      IOException e) {
        Debug.logError(e,module);
      }
    }
    if (bos != null) {
      try {
        bos.close();
      }
 catch (      IOException e) {
        Debug.logError(e,module);
      }
    }
  }
  return bytes;
}
