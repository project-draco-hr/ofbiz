{
  List<Map<String,String>> returnList=null;
  Session session=JCRFactoryUtil.getSession();
  try {
    returnList=getRepositoryNodes(session,startNodePath);
  }
 catch (  RepositoryException e) {
    throw new RepositoryException(e);
  }
 finally {
    session.logout();
  }
  return returnList;
}
