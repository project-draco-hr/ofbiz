{
  if (realMapIfNeeded != null) {
    return realMapIfNeeded.get(obj);
  }
 else {
    for (int i=0; i < names.length; i++) {
      if (obj == null && names[i] == null)       return (V)values[i];
      if (names[i] != null && names[i].equals(obj))       return (V)values[i];
    }
    return null;
  }
}
