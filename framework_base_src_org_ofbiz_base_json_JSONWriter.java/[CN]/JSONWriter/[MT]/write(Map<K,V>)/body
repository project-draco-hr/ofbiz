{
  writer.write('{');
  writer.push();
  Iterator<Map.Entry<K,V>> it=m.entrySet().iterator();
  if (it.hasNext())   writer.newline();
  while (it.hasNext()) {
    Map.Entry<K,V> entry=it.next();
    write(entry.getKey());
    writer.write(':');
    writer.space();
    write(entry.getValue());
    if (it.hasNext())     writer.write(',');
    writer.newline();
  }
  writer.pop();
  writer.write('}');
  return this;
}
