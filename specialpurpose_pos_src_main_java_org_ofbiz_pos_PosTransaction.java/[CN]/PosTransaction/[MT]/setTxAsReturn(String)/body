{
  trace("returned sale");
  txLog.set("statusId","POSTX_RETURNED");
  txLog.set("returnId",returnId);
  txLog.set("logEndDateTime",UtilDateTime.nowTimestamp());
  try {
    txLog.store();
  }
 catch (  GenericEntityException e) {
    Debug.logError(e,"Unable to store TX log - not fatal",module);
  }
  cart.clear();
  currentTx=null;
}
