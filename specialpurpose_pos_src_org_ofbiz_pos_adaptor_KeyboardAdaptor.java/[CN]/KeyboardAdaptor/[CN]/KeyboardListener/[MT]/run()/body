{
  while (running) {
    long now=System.currentTimeMillis();
    if ((lastKey > -1) && (now - lastKey) >= MAX_WAIT.intValue()) {
      this.sendData();
    }
    if (!running) {
      break;
    }
 else {
      try {
        Thread.sleep(MAX_WAIT.intValue());
      }
 catch (      InterruptedException e) {
      }
    }
  }
}
