{
  Boolean processChildren=(Boolean)context.get("processChildren");
  if (processChildren.booleanValue()) {
    appendWhitespace(writer);
    writer.write("</ul>");
  }
  appendWhitespace(writer);
  writer.write("</li>");
  if (node.isRootNode()) {
    appendWhitespace(writer);
    writer.write("</ul>");
    appendWhitespace(writer);
    renderEndingBoundaryComment(writer,"Tree Widget",node.getModelTree());
  }
}
