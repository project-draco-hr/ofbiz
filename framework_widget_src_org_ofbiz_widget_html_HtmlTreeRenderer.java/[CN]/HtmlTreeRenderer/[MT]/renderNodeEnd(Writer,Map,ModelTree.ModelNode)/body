{
  if (node.isRootNode()) {
    appendWhitespace(writer);
    writer.write("</ul>");
    appendWhitespace(writer);
    writer.write("<!-- end tree widget -->");
    appendWhitespace(writer);
  }
 else {
    Boolean processChildren=(Boolean)context.get("processChildren");
    if (processChildren.booleanValue()) {
      appendWhitespace(writer);
      writer.write("</ul>");
    }
    writer.write("</li>");
  }
}
