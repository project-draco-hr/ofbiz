{
  writer.write("<span");
  String id=label.getId(context);
  if (UtilValidate.isNotEmpty(id)) {
    writer.write(" id=\"");
    writer.write(id);
    writer.write("\"");
  }
  String style=label.getStyle(context);
  if (UtilValidate.isNotEmpty(style)) {
    writer.write(" class=\"");
    writer.write(style);
    writer.write("\"");
  }
  writer.write(">");
  writer.write(label.getText(context));
  writer.write("</span>");
  appendWhitespace(writer);
}
