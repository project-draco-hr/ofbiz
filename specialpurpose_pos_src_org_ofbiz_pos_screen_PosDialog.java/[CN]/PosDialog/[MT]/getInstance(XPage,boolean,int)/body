{
  PosDialog dialog=(PosDialog)instances.get(page);
  if (dialog == null) {
synchronized (PosDialog.class) {
      dialog=(PosDialog)instances.get(page);
      if (dialog == null) {
        dialog=new PosDialog(page,modal,padding);
        instances.put(page,dialog);
      }
    }
  }
  dialog.modal=modal;
  dialog.padding=padding;
  dialog.pack();
  return dialog;
}
