{
  FlexibleMapAccessor<Object> fma=new FlexibleMapAccessor<Object>(str);
  Object obj=fma.get(context,locale);
  if (obj != null) {
    try {
      buffer.append((String)ObjectType.simpleTypeConvert(obj,"String",null,timeZone,locale,true));
    }
 catch (    Exception e) {
      buffer.append(obj);
    }
  }
}
