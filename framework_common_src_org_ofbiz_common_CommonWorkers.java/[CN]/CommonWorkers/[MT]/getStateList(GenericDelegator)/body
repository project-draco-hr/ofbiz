{
  List geoList=FastList.newInstance();
  EntityCondition condition=new EntityConditionList(UtilMisc.toList(new EntityExpr("geoTypeId",EntityOperator.EQUALS,"STATE"),new EntityExpr("geoTypeId",EntityOperator.EQUALS,"PROVINCE")),EntityOperator.OR);
  List sortList=UtilMisc.toList("geoName");
  try {
    geoList=delegator.findByConditionCache("Geo",condition,null,sortList);
  }
 catch (  GenericEntityException e) {
    Debug.logError(e,"Cannot lookup State Geos: " + e.toString(),module);
  }
  return geoList;
}
