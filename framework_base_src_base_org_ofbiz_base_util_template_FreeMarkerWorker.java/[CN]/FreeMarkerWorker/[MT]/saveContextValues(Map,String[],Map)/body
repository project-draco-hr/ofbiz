{
  for (int i=0; i < saveKeyNames.length; i++) {
    String key=saveKeyNames[i];
    Object o=context.get(key);
    if (o instanceof Map)     o=UtilMisc.makeMapWritable((Map)o);
 else     if (o instanceof List)     o=UtilMisc.makeListWritable(UtilGenerics.checkList(o));
    saveMap.put(key,o);
  }
}
