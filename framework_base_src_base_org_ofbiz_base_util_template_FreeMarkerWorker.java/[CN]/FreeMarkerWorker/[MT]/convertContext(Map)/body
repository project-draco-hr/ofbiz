{
  Set keySet=ctx.keySet();
  Iterator it=keySet.iterator();
  while (it.hasNext()) {
    String key=(String)it.next();
    Object obj=ctx.get(key);
    if (obj != null) {
      Object unwrappedObj=unwrap(obj);
      if (unwrappedObj != null) {
        ctx.put(key,unwrappedObj);
      }
    }
  }
}
