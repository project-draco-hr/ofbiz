{
  Map saveMap=new HashMap();
  for (int i=0; i < saveKeyNames.length; i++) {
    String key=(String)saveKeyNames[i];
    Object o=context.get(key);
    if (o instanceof Map)     o=new HashMap((Map)o);
 else     if (o instanceof List)     o=new ArrayList((List)o);
    saveMap.put(key,o);
  }
  return saveMap;
}
