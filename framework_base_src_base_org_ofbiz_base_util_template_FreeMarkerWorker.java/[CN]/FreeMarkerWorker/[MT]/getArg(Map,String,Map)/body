{
  Object o=null;
  String returnVal=null;
  o=args.get(key);
  returnVal=(String)unwrap(o);
  if (returnVal == null) {
    try {
      if (templateContext != null) {
        returnVal=(String)templateContext.get(key);
      }
    }
 catch (    ClassCastException e2) {
    }
  }
  return returnVal;
}
