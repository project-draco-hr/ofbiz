{
  StringBuilder retVal=new StringBuilder();
  Set keySet=keySet();
  Iterator keys=keySet.iterator();
  while (keys.hasNext()) {
    String key=keys.next().toString();
    String value=getProperty(key);
    retVal.append(key);
    retVal.append("=");
    retVal.append(value);
    retVal.append("\n");
  }
  return retVal.toString();
}
