{
  if (pk == null) {
    return null;
  }
  HashMap<GenericPK,GenericValue> entityCache=cache.get(pk.getEntityName());
  if (entityCache == null) {
    return null;
  }
  GenericValue value=entityCache.get(pk);
  if (value == null) {
    return null;
  }
 else {
    return (GenericValue)value.clone();
  }
}
