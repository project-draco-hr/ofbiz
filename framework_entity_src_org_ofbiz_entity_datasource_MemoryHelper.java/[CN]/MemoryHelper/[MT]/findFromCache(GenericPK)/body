{
  if (pk == null) {
    return null;
  }
  HashMap entityCache=(HashMap)cache.get(pk.getEntityName());
  if (entityCache == null) {
    return null;
  }
  GenericValue value=(GenericValue)entityCache.get(pk);
  if (value == null) {
    return null;
  }
 else {
    return (GenericValue)value.clone();
  }
}
