{
  Interpreter bsh=getMasterInterpreter(null);
  if (context != null) {
    Set keySet=context.keySet();
    Iterator i=keySet.iterator();
    while (i.hasNext()) {
      Object key=i.next();
      Object value=context.get(key);
      bsh.set((String)key,value);
    }
    bsh.set("context",context);
  }
  return bsh;
}
