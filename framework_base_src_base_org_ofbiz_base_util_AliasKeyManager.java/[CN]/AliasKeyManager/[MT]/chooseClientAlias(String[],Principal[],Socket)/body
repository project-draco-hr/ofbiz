{
  for (int i=0; i < keyType.length; i++) {
    String[] aliases=keyManager.getClientAliases(keyType[i],issuers);
    if (aliases != null && aliases.length > 0) {
      for (int x=0; x < aliases.length; x++) {
        if (alias.equals(aliases[i])) {
          return alias;
        }
      }
    }
  }
  return null;
}
