{
  WfExecutionObject obj=getExecutionObject(workEffortId);
  if (obj != null) {
    Map oCtx=obj.processContext();
    oCtx.putAll(append);
    obj.setProcessContext(oCtx);
    if (Debug.verboseOn())     Debug.logVerbose("ProcessContext (" + workEffortId + ") => "+ obj.processContext(),module);
  }
}
