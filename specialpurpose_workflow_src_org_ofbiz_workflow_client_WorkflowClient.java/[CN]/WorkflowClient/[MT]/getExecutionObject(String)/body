{
  WfExecutionObject obj=null;
  try {
    obj=(WfExecutionObject)WfFactory.getWfActivity(delegator,workEffortId);
  }
 catch (  WfException e) {
  }
  if (obj == null) {
    try {
      obj=(WfExecutionObject)WfFactory.getWfProcess(delegator,workEffortId);
    }
 catch (    WfException e) {
    }
  }
  return obj;
}
