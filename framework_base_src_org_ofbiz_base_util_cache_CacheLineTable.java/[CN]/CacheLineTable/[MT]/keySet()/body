{
  Set<K> keys=new HashSet<K>();
  if (fileTable != null) {
    try {
      addAllFileTableKeys(keys);
    }
 catch (    IOException e) {
      Debug.logError(e,module);
    }
  }
 else {
    keys.addAll(memoryTable.keySet());
    if (isNullSet)     keys.add(null);
  }
  return Collections.unmodifiableSet(keys);
}
