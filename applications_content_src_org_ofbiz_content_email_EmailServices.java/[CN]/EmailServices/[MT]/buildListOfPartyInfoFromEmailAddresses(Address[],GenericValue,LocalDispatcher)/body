{
  InternetAddress emailAddress=null;
  Address addr=null;
  Map map=null;
  Map result=null;
  List tempResults=new ArrayList();
  if (addresses != null) {
    for (int i=0; i < addresses.length; i++) {
      addr=addresses[i];
      if (addr instanceof InternetAddress) {
        emailAddress=(InternetAddress)addr;
        if (!UtilValidate.isEmpty(emailAddress)) {
          result=dispatcher.runSync("findPartyFromEmailAddressCompany",UtilMisc.toMap("address",emailAddress.getAddress(),"userLogin",userLogin));
          if (result.get("partyId") != null) {
            tempResults.add(result);
          }
        }
      }
    }
  }
  return tempResults;
}
