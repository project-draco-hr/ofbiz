{
  if (start.getClass() != end.getClass()) {
    throw new IllegalArgumentException("start Class and end Class must be the same");
  }
  if (end.compareTo((T)start) >= 0) {
    this.start=(T)start;
    this.end=(T)end;
  }
 else {
    this.start=(T)end;
    this.end=(T)start;
  }
  this.isPoint=start.equals(end);
}
