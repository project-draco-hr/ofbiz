{
  UtilTimer timer=(UtilTimer)staticTimers.get(timerName);
  if (timer == null) {
synchronized (UtilTimer.class) {
      timer=(UtilTimer)staticTimers.get(timerName);
      if (timer == null) {
        timer=new UtilTimer(timerName,false);
        timer.setLog(log);
        staticTimers.put(timerName,timer);
      }
    }
  }
  return timer;
}
