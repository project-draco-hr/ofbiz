{
  if (UtilValidate.isEmpty(original) || "null".equals(original)) {
    return nullFma;
  }
  FlexibleMapAccessor fma=fmaCache.get(original);
  if (fma == null) {
synchronized (fmaCache) {
      fmaCache.put(original,new FlexibleMapAccessor(original));
      fma=fmaCache.get(original);
    }
  }
  return fma;
}
