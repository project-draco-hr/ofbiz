{
  if (original == null || original.length() == 0) {
    return nullFma;
  }
  FlexibleMapAccessor fma=fmaCache.get(original);
  if (fma == null) {
synchronized (fmaCache) {
      fma=fmaCache.get(original);
      if (fma == null) {
        fma=new FlexibleMapAccessor(original);
        fmaCache.put(original,fma);
      }
    }
  }
  return fma;
}
