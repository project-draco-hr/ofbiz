{
  if (list != null) {
    if (child == null) {
      return list.remove(index);
    }
 else {
      Map<String,Object> newBase=null;
      try {
        newBase=UtilGenerics.<Map<String,Object>>cast(list.get(index));
      }
 catch (      Exception e) {
      }
      if (newBase != null) {
        return child.remove(context,newBase);
      }
    }
  }
  return null;
}
