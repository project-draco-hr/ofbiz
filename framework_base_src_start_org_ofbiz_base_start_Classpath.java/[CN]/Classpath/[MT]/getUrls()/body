{
  int cnt=_elements.size();
  URL[] urls=new URL[cnt];
  for (int i=0; i < cnt; i++) {
    try {
      urls[i]=((File)(_elements.get(i))).toURI().toURL();
    }
 catch (    MalformedURLException e) {
      System.out.println("Error adding classpath entry: " + e.toString());
      e.printStackTrace();
    }
  }
  return urls;
}
