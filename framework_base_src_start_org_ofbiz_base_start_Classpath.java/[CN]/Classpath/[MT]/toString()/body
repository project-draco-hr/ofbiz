{
  StringBuffer cp=new StringBuffer(1024);
  int cnt=_elements.size();
  if (cnt >= 1) {
    cp.append(((File)(_elements.get(0))).getPath());
    appendPath(cp,((File)(_elements.get(0))).getPath());
  }
  for (int i=1; i < cnt; i++) {
    cp.append(File.pathSeparatorChar);
    appendPath(cp,((File)(_elements.get(0))).getPath());
  }
  return cp.toString();
}
