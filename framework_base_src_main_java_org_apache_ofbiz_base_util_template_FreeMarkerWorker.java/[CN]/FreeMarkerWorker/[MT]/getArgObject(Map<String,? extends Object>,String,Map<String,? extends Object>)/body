{
  Object o=args.get(key);
  Object returnVal=unwrap(o);
  if (returnVal == null) {
    try {
      if (templateContext != null) {
        returnVal=templateContext.get(key);
      }
    }
 catch (    ClassCastException e2) {
      Debug.logInfo(e2.getMessage(),module);
    }
  }
  return returnVal;
}
