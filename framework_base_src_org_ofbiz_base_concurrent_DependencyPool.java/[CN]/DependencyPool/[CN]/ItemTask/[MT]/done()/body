{
  super.done();
  results.put(item.getKey(),this);
  provides.addAll(item.getProvides());
  submitLock.lock();
  try {
    outstanding.remove(item);
    if (outstanding.size() < inflight && submitWork() == 0 && outstanding.isEmpty()) {
      submitCondition.signal();
    }
  }
  finally {
    submitLock.unlock();
  }
}
