{
  int s1=0, s2=-1;
  List out=new ArrayList(40);
  out.add("empty");
  while (true) {
    s2=r.indexOf(delim,s1);
    if (s2 != -1) {
      out.add(r.substring(s1,s2));
    }
 else {
      String _=r.substring(s1);
      if (_ != null && !_.equals("")) {
        out.add(_);
      }
      break;
    }
    s1=s2;
    s1+=delim.length();
  }
  return out;
}
