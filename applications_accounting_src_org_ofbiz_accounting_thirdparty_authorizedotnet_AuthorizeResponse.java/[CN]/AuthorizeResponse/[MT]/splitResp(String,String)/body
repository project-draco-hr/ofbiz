{
  int s1=0, s2=-1;
  Vector out=new Vector(40);
  out.addElement("empty");
  while (true) {
    s2=r.indexOf(delim,s1);
    if (s2 != -1) {
      out.addElement(r.substring(s1,s2));
    }
 else {
      String _=r.substring(s1);
      if (_ != null && !_.equals("")) {
        out.addElement(_);
      }
      break;
    }
    s1=s2;
    s1+=delim.length();
  }
  return out;
}
