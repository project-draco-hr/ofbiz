{
  String rc=response.get(RESPONSE_CODE);
  if (rc.equals("1")) {
    this.respCode=APPROVED;
  }
  if (rc.equals("2")) {
    this.respCode=DECLINED;
  }
  if (rc.equals("3")) {
    this.respCode=ERROR;
  }
  this.reasonCode=response.get(RESPONSE_REASON_CODE);
  this.reasonText=response.get(RESPONSE_REASON_TEXT);
}
