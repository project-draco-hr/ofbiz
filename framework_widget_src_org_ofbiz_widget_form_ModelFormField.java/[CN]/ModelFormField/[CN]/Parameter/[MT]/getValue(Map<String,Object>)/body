{
  if (this.value != null) {
    return this.value.expandString(context);
  }
 else   if (this.fromField != null) {
    Object contextVal=this.fromField.get(context);
    return contextVal.toString();
  }
 else {
    Object obj=context.get(this.name);
    if (obj != null) {
      return obj.toString();
    }
 else {
      return null;
    }
  }
}
