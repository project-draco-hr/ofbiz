{
  StringBuffer strBufTemp=new StringBuffer(strTestBase);
  if (iNum < 10000) {
    strBufTemp.append("0");
  }
  if (iNum < 1000) {
    strBufTemp.append("0");
  }
  if (iNum < 100) {
    strBufTemp.append("0");
  }
  if (iNum < 10) {
    strBufTemp.append("0");
  }
  strBufTemp.append(iNum);
  return strBufTemp.toString();
}
