{
  String freq=rule.getString("frequency");
  if (freq == null)   return 0;
  if (freq.equalsIgnoreCase("SECONDLY"))   return SECONDLY;
  if (freq.equalsIgnoreCase("MINUTELY"))   return MINUTELY;
  if (freq.equalsIgnoreCase("HOURLY"))   return HOURLY;
  if (freq.equalsIgnoreCase("DAILY"))   return DAILY;
  if (freq.equalsIgnoreCase("WEEKLY"))   return WEEKLY;
  if (freq.equalsIgnoreCase("MONTHLY"))   return MONTHLY;
  if (freq.equalsIgnoreCase("YEARLY"))   return YEARLY;
  return 0;
}
