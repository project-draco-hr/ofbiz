{
  Iterator iter=thisCollection.iterator();
  while (iter.hasNext()) {
    try {
      GenericValue related=((GenericValue)iter.next()).getRelatedOne(typeRelation);
      if (isType(related,targetType)) {
        return true;
      }
    }
 catch (    GenericEntityException e) {
      continue;
    }
  }
  return false;
}
