{
  if (wasMouseClicked()) {
    String text="";
    try {
      text=m_edit.getText();
    }
 catch (    NullPointerException e) {
      text="";
    }
 finally {
      text=c + text;
    }
    if (countChars(text,c) > 1) {
      text=stripChars(text,c);
    }
    m_edit.setText(text);
    m_dialog.repaint();
    return;
  }
}
