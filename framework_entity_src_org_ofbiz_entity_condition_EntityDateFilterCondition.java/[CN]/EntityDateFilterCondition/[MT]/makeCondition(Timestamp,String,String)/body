{
  return new EntityExpr(new EntityExpr(new EntityExpr(thruDateName,EntityOperator.EQUALS,null),EntityOperator.OR,new EntityExpr(thruDateName,EntityOperator.GREATER_THAN,moment)),EntityOperator.AND,new EntityExpr(new EntityExpr(fromDateName,EntityOperator.EQUALS,null),EntityOperator.OR,new EntityExpr(fromDateName,EntityOperator.LESS_THAN_EQUAL_TO,moment)));
}
