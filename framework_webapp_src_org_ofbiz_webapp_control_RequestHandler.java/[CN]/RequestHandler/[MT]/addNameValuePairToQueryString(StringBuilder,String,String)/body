{
  if (UtilValidate.isNotEmpty(value)) {
    if (queryString.length() > 1) {
      queryString.append("&");
    }
    String encodedName=StringUtil.getEncoder("url").encode(name);
    if (encodedName != null) {
      queryString.append(encodedName);
      queryString.append("=");
      queryString.append(StringUtil.getEncoder("url").encode(value));
    }
  }
}
