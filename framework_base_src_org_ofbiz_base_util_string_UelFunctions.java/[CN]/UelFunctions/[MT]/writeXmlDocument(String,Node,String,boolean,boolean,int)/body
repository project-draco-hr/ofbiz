{
  try {
    File file=FileUtil.getFile(str);
    if (file != null) {
      FileOutputStream os=new FileOutputStream(file);
      UtilXml.writeXmlDocument(node,os,encoding,omitXmlDeclaration,indent,indentAmount,false);
      os.close();
      return true;
    }
 else {
      Debug.logError("Unable to create XML document " + str,module);
    }
  }
 catch (  Exception e) {
    Debug.logError(e,"Error while writing XML document " + str,module);
  }
  return false;
}
