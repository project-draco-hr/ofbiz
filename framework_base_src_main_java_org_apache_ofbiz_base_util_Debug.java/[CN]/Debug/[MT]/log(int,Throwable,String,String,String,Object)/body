{
  if (isOn(level)) {
    if (msg != null && params.length > 0) {
      StringBuilder sb=new StringBuilder();
      Formatter formatter=new Formatter(sb);
      formatter.format(msg,params);
      msg=sb.toString();
    }
    Logger logger=getLogger(module);
    logger.log(levelObjs[level],msg,t);
  }
}
