{
  buffer.append("<fo:table>");
  this.appendWhitespace(buffer);
  buffer.append("<fo:table-column column-width=\"2in\"/>");
  this.appendWhitespace(buffer);
  buffer.append("<fo:table-column/>");
  this.appendWhitespace(buffer);
  buffer.append("<fo:table-body>");
  this.appendWhitespace(buffer);
}
