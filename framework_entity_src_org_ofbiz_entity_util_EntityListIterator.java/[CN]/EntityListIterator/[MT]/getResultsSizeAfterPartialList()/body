{
  if (genericDAO != null) {
    if (resultSize == null) {
      EntityFindOptions efo=null;
      if (distinctQuery) {
        efo=new EntityFindOptions();
        efo.setDistinct(distinctQuery);
      }
      resultSize=(int)genericDAO.selectCountByCondition(modelEntity,whereCondition,havingCondition,efo);
    }
    return resultSize;
  }
 else   if (this.last()) {
    return this.currentIndex();
  }
 else {
    return 0;
  }
}
