{
  Object converted;
  try {
    converted=ObjectType.simpleTypeConvert(value,type,null,null,false);
  }
 catch (  Exception e) {
    Debug.logWarning("Service [" + serviceName + "] attribute ["+ name+ "] default value could not be converted to type ["+ type+ "]",module);
    return value;
  }
  return converted;
}
