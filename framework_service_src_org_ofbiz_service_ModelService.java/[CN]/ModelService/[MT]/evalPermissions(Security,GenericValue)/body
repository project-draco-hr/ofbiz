{
  if (this.containsPermissions()) {
    Iterator i=this.permissionGroups.iterator();
    while (i.hasNext()) {
      ModelPermGroup group=(ModelPermGroup)i.next();
      if (!group.evalPermissions(security,userLogin)) {
        return false;
      }
    }
    return true;
  }
 else {
    return true;
  }
}
