{
  Map<String,Object> uri=getRequestMapMap(uriStr);
  if (uri != null)   return (String)((Map<String,String>)uri.get(ConfigXMLReader.RESPONSE_MAP)).get(ConfigXMLReader.NEXT_PAGE_DEFAULT);
 else {
    Debug.logWarning("[RequestManager.getViewName] View name for uri \"" + uriStr + "\" not found",module);
    return null;
  }
}
