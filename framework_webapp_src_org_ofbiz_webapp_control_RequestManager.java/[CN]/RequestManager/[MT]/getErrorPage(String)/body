{
  Map uri=getRequestMapMap(uriStr);
  if (uri != null) {
    String errorViewUri=(String)uri.get(ConfigXMLReader.ERROR_PAGE);
    String returnPage=getViewPage(errorViewUri);
    if (returnPage != null) {
      return returnPage;
    }
 else {
      return getDefaultErrorPage();
    }
  }
 else {
    return getDefaultErrorPage();
  }
}
