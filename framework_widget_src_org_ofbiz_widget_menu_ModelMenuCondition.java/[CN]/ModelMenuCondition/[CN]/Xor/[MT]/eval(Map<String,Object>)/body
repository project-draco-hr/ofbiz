{
  boolean foundOneTrue=false;
  Iterator subConditionIter=this.subConditions.iterator();
  while (subConditionIter.hasNext()) {
    MenuCondition subCondition=(MenuCondition)subConditionIter.next();
    if (subCondition.eval(context)) {
      if (foundOneTrue) {
        return false;
      }
 else {
        foundOneTrue=true;
      }
    }
  }
  return foundOneTrue;
}
