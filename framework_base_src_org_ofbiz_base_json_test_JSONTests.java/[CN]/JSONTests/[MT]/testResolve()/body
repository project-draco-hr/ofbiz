{
  assertResolveJSON("url",new URL("http://ofbiz.apache.org"),"resolve(\"java.net.URL:http:\\/\\/ofbiz.apache.org\")");
  IOException caught=null;
  try {
    getJSON(new URL("http://ofbiz.apache.org"),false);
  }
 catch (  IOException e) {
    caught=e;
  }
 finally {
    assertNotNull("url not allowed",caught);
  }
}
