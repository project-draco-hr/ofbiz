{
  if (deviceName != null && control != null) {
    if (!"[NOT IMPLEMENTED]".equals(deviceName) && !"[DISABLED]".equals(deviceName)) {
      control.open(deviceName);
      control.claim(timeout);
      this.enable(true);
      this.initialize();
    }
  }
 else {
    Debug.logWarning("No device named [" + deviceName + "] available",module);
  }
}
