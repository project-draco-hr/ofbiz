{
  Converter<JSON,Set> converter=Converters.getConverter(JSON.class,Set.class);
  JSON json;
  Set set, convertedSet;
  set=new TreeSet();
  set.add("field1");
  set.add("field2");
  json=JSONArray.fromObject(set);
  convertedSet=converter.convert(json);
  assertEquals("JSON to Set",set,convertedSet);
}
