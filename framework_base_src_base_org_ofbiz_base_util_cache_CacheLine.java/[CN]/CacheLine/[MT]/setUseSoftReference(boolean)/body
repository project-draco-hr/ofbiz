{
  if (this.useSoftReference != useSoftReference) {
synchronized (this) {
      this.useSoftReference=useSoftReference;
      if (useSoftReference) {
        this.valueRef=new CacheSoftReference(this.valueRef);
      }
 else {
        this.valueRef=((CacheSoftReference)this.valueRef).get();
      }
    }
  }
}
