{
  List props=new LinkedList();
  if (properties != null && properties.size() > 0) {
    Iterator i=properties.entrySet().iterator();
    while (i.hasNext()) {
      Map.Entry e=(Map.Entry)i.next();
      Property p=(Property)e.getValue();
      if (p != null && value.equals(p.value)) {
        props.add(p);
      }
    }
  }
  return props;
}
