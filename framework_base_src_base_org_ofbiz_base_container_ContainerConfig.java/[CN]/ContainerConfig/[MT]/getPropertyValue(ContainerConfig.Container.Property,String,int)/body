{
  ContainerConfig.Container.Property prop=parentProp.getProperty(name);
  if (prop == null || UtilValidate.isEmpty(prop.value)) {
    return defaultValue;
  }
 else {
    int num=defaultValue;
    try {
      num=Integer.parseInt(prop.value);
    }
 catch (    Exception e) {
      return defaultValue;
    }
    return num;
  }
}
