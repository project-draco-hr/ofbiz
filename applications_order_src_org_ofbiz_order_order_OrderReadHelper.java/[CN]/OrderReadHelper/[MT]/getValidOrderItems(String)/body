{
  if (shipGroupSeqId == null)   return getValidOrderItems();
  List exprs=UtilMisc.toList(new EntityExpr("statusId",EntityOperator.NOT_EQUAL,"ITEM_CANCELLED"),new EntityExpr("statusId",EntityOperator.NOT_EQUAL,"ITEM_REJECTED"),new EntityExpr("shipGroupSeqId",EntityOperator.EQUALS,shipGroupSeqId));
  return EntityUtil.filterByAnd(getOrderItemAndShipGroupAssoc(),exprs);
}
