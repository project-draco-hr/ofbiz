{
  if (orderItem == null)   return null;
  try {
    return orderHeader.getDelegator().findByAnd("OrderItemShipGroupAssoc",UtilMisc.toMap("orderId",orderItem.getString("orderId"),"orderItemSeqId",orderItem.getString("orderItemSeqId")),UtilMisc.toList("shipGroupSeqId"));
  }
 catch (  GenericEntityException e) {
    Debug.logWarning(e,module);
  }
  return null;
}
