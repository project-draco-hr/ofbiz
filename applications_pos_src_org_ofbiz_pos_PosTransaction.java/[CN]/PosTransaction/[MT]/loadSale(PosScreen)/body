{
  List shoppingLists=createShoppingLists();
  if (!shoppingLists.isEmpty()) {
    Hashtable salesMap=createSalesMap(shoppingLists);
    if (!salesMap.isEmpty()) {
      LoadSale loadSale=new LoadSale(salesMap,this,pos);
      loadSale.openDlg();
    }
 else {
      pos.showDialog("dialog/error/nosales");
    }
  }
 else {
    pos.showDialog("dialog/error/nosales");
  }
}
