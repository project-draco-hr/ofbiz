{
  if (permissions != null && permissions.size() > 0) {
    boolean foundOne=false;
    for (    ModelPermission perm : permissions) {
      if (perm.evalPermission(dctx,context)) {
        foundOne=true;
      }
 else {
        if (joinType.equals(PERM_JOIN_AND)) {
          return false;
        }
      }
    }
    return foundOne;
  }
 else {
    return true;
  }
}
