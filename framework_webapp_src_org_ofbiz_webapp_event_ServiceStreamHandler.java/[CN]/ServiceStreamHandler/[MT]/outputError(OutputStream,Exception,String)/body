{
  PrintStream out=new PrintStream(stream);
  if (message != null)   out.println("Error message: " + message);
  if (error != null)   out.println("Exception occured: " + error.toString());
  out.flush();
  out.close();
  try {
    stream.close();
  }
 catch (  IOException e) {
    Debug.logError(e,module);
  }
}
