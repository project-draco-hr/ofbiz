{
  List<String> splitList=null;
  StringTokenizer st;
  if (str == null)   return null;
  st=(delim != null ? new StringTokenizer(str,delim) : new StringTokenizer(str));
  if (st.hasMoreTokens()) {
    splitList=new LinkedList<>();
    while (st.hasMoreTokens())     splitList.add(st.nextToken());
  }
  return splitList;
}
