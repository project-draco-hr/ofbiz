{
  ModelServiceReader reader;
  reader=(ModelServiceReader)readersLoader.get(handler);
  if (reader == null) {
synchronized (ModelServiceReader.class) {
      reader=(ModelServiceReader)readersLoader.get(handler);
      if (reader == null) {
        reader=new ModelServiceReader(handler,dctx);
        readersLoader.put(handler,reader);
      }
    }
  }
  return reader;
}
