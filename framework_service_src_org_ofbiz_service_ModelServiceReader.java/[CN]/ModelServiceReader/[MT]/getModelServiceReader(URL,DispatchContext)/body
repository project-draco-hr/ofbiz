{
  ModelServiceReader reader=null;
  reader=(ModelServiceReader)readersUrl.get(readerURL);
  if (reader == null) {
synchronized (ModelServiceReader.class) {
      reader=(ModelServiceReader)readersUrl.get(readerURL);
      if (reader == null) {
        reader=new ModelServiceReader(readerURL,dctx);
        readersUrl.put(readerURL,reader);
      }
    }
  }
  return reader;
}
