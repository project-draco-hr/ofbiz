{
  try {
    try {
      return getClass().getConstructor(ModelService.class).newInstance(modelService);
    }
 catch (    InvocationTargetException e) {
      throw e.getCause();
    }
  }
 catch (  RuntimeException e) {
    throw e;
  }
catch (  Error e) {
    throw e;
  }
catch (  Throwable e) {
    throw (InternalError)new InternalError(e.getMessage()).initCause(e);
  }
}
