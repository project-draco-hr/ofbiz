{
  String a1=null;
  String a2=null;
  try {
    a1=as1.activity().key();
    a2=as2.activity().key();
  }
 catch (  BaseException e) {
    throw new IllegalArgumentException("Unable to obtain activity from assignment");
  }
  if (order == ASCENDING_ORDER) {
    return a1.compareTo(a2);
  }
 else {
    return a2.compareTo(a1);
  }
}
