{
  this.initJetty();
  if (servers != null) {
    Iterator i=servers.values().iterator();
    while (i.hasNext()) {
      Server server=(Server)i.next();
      try {
        server.start();
      }
 catch (      MultiException e) {
        Debug.logError(e,"Jetty Server Multi-Exception",module);
        throw new ContainerException(e);
      }
    }
  }
  return true;
}
