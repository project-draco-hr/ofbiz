{
  GenericValue securityGroupPermissionValue=delegator.makeValue("SecurityGroupPermission",UtilMisc.toMap("groupId",groupId,"permissionId",permission));
  Boolean exists=(Boolean)securityGroupPermissionCache.get(securityGroupPermissionValue);
  if (exists == null) {
    try {
      if (delegator.findByPrimaryKey(securityGroupPermissionValue.getPrimaryKey()) != null)       exists=Boolean.TRUE;
 else       exists=Boolean.FALSE;
    }
 catch (    GenericEntityException e) {
      exists=Boolean.FALSE;
      Debug.logWarning(e,module);
    }
    securityGroupPermissionCache.put(securityGroupPermissionValue,exists);
  }
  return exists.booleanValue();
}
