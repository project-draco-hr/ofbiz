{
  String minString=getElementAttr("thread-pool","failed-retry-min");
  int retryMin;
  try {
    retryMin=Integer.parseInt(minString);
  }
 catch (  NumberFormatException e) {
    Debug.logError(e,"Unable to parse retry minutes; using default of 30",module);
    retryMin=30;
  }
  return retryMin;
}
