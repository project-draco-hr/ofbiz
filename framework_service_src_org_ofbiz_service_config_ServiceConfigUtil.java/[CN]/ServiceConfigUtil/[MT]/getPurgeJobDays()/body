{
  String days=getElementAttr("thread-pool","purge-job-days");
  int purgeDays;
  try {
    purgeDays=Integer.parseInt(days);
  }
 catch (  NumberFormatException e) {
    Debug.logError(e,"Cannot read the number of days to keep jobs; not purging",module);
    purgeDays=0;
  }
  return purgeDays;
}
