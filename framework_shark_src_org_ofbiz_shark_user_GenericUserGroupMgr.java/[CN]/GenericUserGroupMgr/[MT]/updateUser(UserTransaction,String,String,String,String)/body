{
  GenericValue user=getUser(username);
  if (user != null) {
    user.set("firstName",firstName);
    user.set("lastName",firstName);
    user.set("emailAddress",email);
    try {
      user.store();
    }
 catch (    GenericEntityException e) {
      Debug.logError(e,module);
      throw new RootException(e);
    }
  }
}
