{
  Iterator<Map.Entry<Object,CacheLine<V>>> it=memoryTable.entrySet().iterator();
  while (it.hasNext()) {
    Map.Entry<Object,CacheLine<V>> entry=it.next();
    CacheLine<V> line=entry.getValue();
    if (line.isInvalid()) {
      it.remove();
      postRemove(toKey(entry.getKey()),line.getValue(),false);
    }
  }
}
