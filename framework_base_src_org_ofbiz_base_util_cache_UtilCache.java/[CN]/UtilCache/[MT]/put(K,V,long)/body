{
  CacheLine<V> oldCacheLine=putInternal(fromKey(key),createCacheLine(value,expireTime));
  V oldValue=oldCacheLine == null ? null : oldCacheLine.getValue();
  if (oldValue == null) {
    noteAddition(key,value);
    return null;
  }
 else {
    noteUpdate(key,value,oldValue);
    return oldValue;
  }
}
