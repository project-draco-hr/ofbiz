{
  long totalSize=0;
  if (fileTable != null) {
    try {
      FastIterator<CacheLine<V>> iter=fileTable.values();
      CacheLine<V> value=iter.next();
      while (value != null) {
        totalSize+=value.getSizeInBytes();
        value=iter.next();
      }
    }
 catch (    IOException e) {
      Debug.logError(e,module);
      return 0;
    }
  }
 else {
    for (    CacheLine<V> line : memoryTable.values()) {
      totalSize+=line.getSizeInBytes();
    }
  }
  return totalSize;
}
