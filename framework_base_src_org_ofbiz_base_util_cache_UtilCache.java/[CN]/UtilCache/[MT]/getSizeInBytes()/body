{
  long totalSize=0;
  if (fileTable != null) {
    try {
      FastIterator<V> iter=fileTable.values();
      V value=iter.next();
      while (value != null) {
        totalSize+=findSizeInBytes(value);
        value=iter.next();
      }
    }
 catch (    IOException e) {
      Debug.logError(e,module);
      return 0;
    }
  }
 else {
    for (    CacheLine<V> line : memoryTable.values()) {
      totalSize+=findSizeInBytes(line.getValue());
    }
  }
  return totalSize;
}
