{
  if (fileTable != null) {
    int size=0;
    try {
      FastIterator<Object> iter=fileTable.keys();
      while (iter.next() != null) {
        size++;
      }
    }
 catch (    IOException e) {
      Debug.logError(e,module);
    }
    return size;
  }
 else {
    return memoryTable.size();
  }
}
