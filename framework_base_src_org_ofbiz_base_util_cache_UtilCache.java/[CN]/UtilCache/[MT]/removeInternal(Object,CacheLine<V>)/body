{
  Object nulledKey=fromKey(key);
  cancel(existingCacheLine);
  if (!memoryTable.remove(nulledKey,existingCacheLine)) {
    return;
  }
  if (fileTable != null) {
    try {
synchronized (this) {
        fileTable.remove(nulledKey);
        jdbmMgr.commit();
      }
    }
 catch (    IOException e) {
      Debug.logError(e,module);
    }
  }
  noteRemoval((K)key,existingCacheLine.getValue());
}
