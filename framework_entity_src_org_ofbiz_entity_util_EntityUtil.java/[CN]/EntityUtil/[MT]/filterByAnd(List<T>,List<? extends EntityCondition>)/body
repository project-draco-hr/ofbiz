{
  if (values == null)   return null;
  if (exprs == null || exprs.size() == 0) {
    return values;
  }
  List<T> result=FastList.newInstance();
  for (  T value : values) {
    boolean include=true;
    for (    EntityCondition condition : exprs) {
      include=condition.entityMatches(value);
      if (!include)       break;
    }
    if (include) {
      result.add(value);
    }
  }
  return result;
}
