{
  if (values != null) {
    if (values.size() <= 0) {
      return null;
    }
    if (values.size() == 1) {
      return (GenericValue)values.iterator().next();
    }
 else {
      throw new IllegalArgumentException("Passed List had more than one value.");
    }
  }
 else {
    return null;
  }
}
