{
  EntityCondition searchCondition=new EntityConditionList<EntityCondition>(UtilMisc.toList(new EntityFieldMap(search,EntityOperator.AND),EntityUtil.getFilterByDateExpr(now)),EntityOperator.AND);
  return delegator.findList(entityName,searchCondition,null,UtilMisc.toList("-fromDate"),null,false);
}
