{
  StringBuilder buf=new StringBuilder();
  for (  Map.Entry<String,String> entry : document.entrySet()) {
    String name=(String)entry.getKey();
    String value=(String)entry.getValue();
    buf.append(name);
    buf.append(" ");
    buf.append(value);
    buf.append("\r\n");
  }
  buf.append(".\r\n");
  return buf.toString();
}
