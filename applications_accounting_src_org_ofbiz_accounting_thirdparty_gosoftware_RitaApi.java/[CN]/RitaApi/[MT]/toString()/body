{
  StringBuilder buf=new StringBuilder();
  Iterator<Map.Entry<String,String>> i=document.entrySet().iterator();
  while (i.hasNext()) {
    Map.Entry<String,String> entry=i.next();
    String name=(String)entry.getKey();
    String value=(String)entry.getValue();
    buf.append(name);
    buf.append(" ");
    buf.append(value);
    buf.append("\r\n");
  }
  buf.append(".\r\n");
  return buf.toString();
}
