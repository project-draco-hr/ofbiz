{
  StringBuffer buf=new StringBuffer();
  MapIterator i=document.mapIterator();
  while (i.hasNext()) {
    String name=(String)i.next();
    String value=(String)i.getValue();
    buf.append(name);
    buf.append(" ");
    buf.append(value);
    buf.append("\r\n");
  }
  buf.append(".\r\n");
  return buf.toString();
}
