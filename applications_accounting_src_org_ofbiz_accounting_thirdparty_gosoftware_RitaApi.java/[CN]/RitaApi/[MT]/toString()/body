{
  StringBuilder buf=new StringBuilder();
  Iterator i=document.entrySet().iterator();
  while (i.hasNext()) {
    Map.Entry entry=(Map.Entry)i.next();
    String name=(String)entry.getKey();
    String value=(String)entry.getValue();
    buf.append(name);
    buf.append(" ");
    buf.append(value);
    buf.append("\r\n");
  }
  buf.append(".\r\n");
  return buf.toString();
}
