{
  writer.write("<div");
  String id=container.getId(context);
  if (UtilValidate.isNotEmpty(id)) {
    writer.write(" id=\"");
    writer.write(id);
    writer.write("\"");
  }
  String style=container.getStyle(context);
  if (UtilValidate.isNotEmpty(style)) {
    writer.write(" class=\"");
    writer.write(style);
    writer.write("\"");
  }
  writer.write(">");
  appendWhitespace(writer);
}
