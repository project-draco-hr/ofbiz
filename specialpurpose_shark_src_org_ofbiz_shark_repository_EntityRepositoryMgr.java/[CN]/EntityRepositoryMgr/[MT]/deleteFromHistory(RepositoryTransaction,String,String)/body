{
  Debug.logInfo("XPDL Delete from History: " + xpdlId + "/"+ xpdlVersion,module);
  GenericValue value=this.getXpdlValue(xpdlId,xpdlVersion,true);
  if (value != null) {
    try {
      value.remove();
    }
 catch (    GenericEntityException e) {
      throw new RepositoryException(e);
    }
  }
}
