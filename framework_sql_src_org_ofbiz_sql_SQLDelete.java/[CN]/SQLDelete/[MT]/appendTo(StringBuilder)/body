{
  sb.append("DELETE FROM ");
  table.getTableName().appendTo(sb);
  if (table.getJoined() != null) {
    sb.append(" USING");
    table.getJoined().appendToRest(table.getTableName().getAlias(),sb);
  }
  if (whereCondition != null) {
    sb.append(" WHERE ");
    whereCondition.appendTo(sb);
  }
  sb.append(';');
  return sb;
}
