{
  if (freq == null)   return false;
  if (freq.equalsIgnoreCase("SECONDLY"))   return true;
  if (freq.equalsIgnoreCase("MINUTELY"))   return true;
  if (freq.equalsIgnoreCase("HOURLY"))   return true;
  if (freq.equalsIgnoreCase("DAILY"))   return true;
  if (freq.equalsIgnoreCase("WEEKLY"))   return true;
  if (freq.equalsIgnoreCase("MONTHLY"))   return true;
  if (freq.equalsIgnoreCase("YEARLY"))   return true;
  return false;
}
