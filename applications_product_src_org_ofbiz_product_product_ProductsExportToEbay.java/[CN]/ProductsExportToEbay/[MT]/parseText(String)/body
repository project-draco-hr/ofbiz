{
  Pattern htmlPattern=Pattern.compile("[<](.+?)[>]");
  Pattern tabPattern=Pattern.compile("\\s");
  if (null != text && text.length() > 0) {
    Matcher matcher=htmlPattern.matcher(text);
    text=matcher.replaceAll("");
    matcher=tabPattern.matcher(text);
    text=matcher.replaceAll(" ");
  }
 else {
    text="";
  }
  return text;
}
