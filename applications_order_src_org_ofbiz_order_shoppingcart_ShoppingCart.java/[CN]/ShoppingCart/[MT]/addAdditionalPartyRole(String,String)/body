{
  List parties=(List)additionalPartyRole.get(roleTypeId);
  if (parties != null) {
    Iterator it=parties.iterator();
    while (it.hasNext()) {
      if (((String)it.next()).equals(partyId)) {
        return;
      }
    }
  }
 else {
    parties=new LinkedList();
    additionalPartyRole.put(roleTypeId,parties);
  }
  parties.add(0,partyId);
}
