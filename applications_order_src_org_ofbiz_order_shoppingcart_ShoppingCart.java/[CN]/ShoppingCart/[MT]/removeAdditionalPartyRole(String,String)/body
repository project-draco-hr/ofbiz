{
  List parties=(List)additionalPartyRole.get(roleTypeId);
  if (parties != null) {
    Iterator it=parties.iterator();
    while (it.hasNext()) {
      if (((String)it.next()).equals(partyId)) {
        it.remove();
        if (parties.isEmpty()) {
          additionalPartyRole.remove(roleTypeId);
        }
        return;
      }
    }
  }
}
