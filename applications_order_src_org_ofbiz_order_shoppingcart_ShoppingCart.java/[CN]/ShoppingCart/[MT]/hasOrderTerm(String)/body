{
  if (termTypeId == null) {
    return false;
  }
  Iterator orderTermsIt=orderTerms.iterator();
  while (orderTermsIt.hasNext()) {
    GenericValue orderTerm=(GenericValue)orderTermsIt.next();
    if (termTypeId.equals(orderTerm.getString("termTypeId"))) {
      return true;
    }
  }
  return false;
}
