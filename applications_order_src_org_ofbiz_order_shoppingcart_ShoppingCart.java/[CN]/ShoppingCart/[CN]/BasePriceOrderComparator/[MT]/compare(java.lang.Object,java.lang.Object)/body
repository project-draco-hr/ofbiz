{
  ShoppingCartItem cartItem=(ShoppingCartItem)obj;
  ShoppingCartItem cartItem1=(ShoppingCartItem)obj1;
  int compareValue=new Double(cartItem.getBasePrice()).compareTo(new Double(cartItem1.getBasePrice()));
  if (this.ascending) {
    return compareValue;
  }
 else {
    return -compareValue;
  }
}
