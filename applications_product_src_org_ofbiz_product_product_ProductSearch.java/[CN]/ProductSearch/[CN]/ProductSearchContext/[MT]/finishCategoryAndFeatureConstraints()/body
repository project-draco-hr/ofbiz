  }
  if (includeFeatureIdOrSetAndList.size() > 0) {
    Iterator includeFeatureIdOrSetAndIter=includeFeatureIdOrSetAndList.iterator();
    while (includeFeatureIdOrSetAndIter.hasNext()) {
