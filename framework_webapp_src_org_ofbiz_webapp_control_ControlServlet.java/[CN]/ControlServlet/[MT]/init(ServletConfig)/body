{
  super.init(config);
  if (Debug.infoOn()) {
    ServletContext servletContext=config.getServletContext();
    Debug.logInfo("LOADING WEBAPP [" + servletContext.getContextPath().substring(1) + "] "+ servletContext.getServletContextName()+ ", located at "+ servletContext.getRealPath("/"),module);
  }
  configureBsf();
  getRequestHandler();
}
