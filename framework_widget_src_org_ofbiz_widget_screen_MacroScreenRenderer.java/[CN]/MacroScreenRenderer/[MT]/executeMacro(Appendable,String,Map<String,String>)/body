{
  StringBuilder sb=new StringBuilder("<@");
  sb.append(macroName);
  if (parameters != null) {
    for (    Map.Entry<String,String> parameter : parameters.entrySet()) {
      sb.append(' ');
      sb.append(parameter.getKey());
      sb.append("=\"");
      sb.append(parameter.getValue().replaceAll("\"","\\\\\""));
      sb.append('"');
    }
  }
  sb.append(" />");
  executeMacro(writer,sb.toString());
}
