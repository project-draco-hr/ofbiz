{
  String statesArr[]={"open.running","open.not_running.not_started","open.not_running.suspended","closed.completed","closed.terminated","closed.aborted"};
  String entityArr[]={"WF_RUNNING","WF_NOT_STARTED","WF_SUSPENDED","WF_COMPLETED","WF_TERMINATED","WF_ABORTED"};
  for (int i=0; i < entityArr.length; i++) {
    if (entityArr[i].equals(state))     return statesArr[i];
  }
  return null;
}
