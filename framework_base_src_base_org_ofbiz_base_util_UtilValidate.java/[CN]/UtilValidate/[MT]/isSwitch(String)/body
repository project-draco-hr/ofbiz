{
  String first4digs=cc.substring(0,4);
  String first6digs=cc.substring(0,6);
  if (((cc.length() == 16) || (cc.length() == 18) || (cc.length() == 19)) && (first4digs.equals("4903") || first4digs.equals("4905") || first4digs.equals("4911")|| first4digs.equals("4936")|| first6digs.equals("564182")|| first6digs.equals("633110")|| first4digs.equals("6333")|| first4digs.equals("6759")))   return isCreditCard(cc);
  return false;
}
