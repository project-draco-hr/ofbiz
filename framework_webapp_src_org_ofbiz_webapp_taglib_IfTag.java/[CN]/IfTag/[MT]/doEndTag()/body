{
  try {
    BodyContent body=getBodyContent();
    if (body != null) {
      JspWriter out=body.getEnclosingWriter();
      String bodyString=body.getString();
      body.clearBody();
      out.print(bodyString);
    }
  }
 catch (  IOException e) {
    Debug.logError(e,"IfTag Error.",module);
  }
  return EVAL_PAGE;
}
