{
  try {
    java.lang.Thread.sleep(30000);
  }
 catch (  InterruptedException e) {
  }
  while (!executor.isShutdown()) {
    try {
      List<Job> pollList=jm.poll();
      for (      Job job : pollList) {
        if (job.isValid()) {
          queueNow(job);
        }
      }
      java.lang.Thread.sleep(pollWaitTime());
    }
 catch (    InterruptedException e) {
      Debug.logError(e,module);
      stop();
    }
  }
}
