{
  Comparable thisVal=(Comparable)this.fields.get(name);
  Comparable thatVal=(Comparable)that.fields.get(name);
  if (thisVal == null) {
    if (thatVal == null)     return 0;
 else     return 1;
  }
 else {
    if (thatVal == null)     return -1;
 else     return thisVal.compareTo(thatVal);
  }
}
