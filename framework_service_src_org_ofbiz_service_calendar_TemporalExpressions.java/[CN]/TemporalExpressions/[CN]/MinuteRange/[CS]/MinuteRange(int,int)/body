{
  if (start < 0 || start > 23) {
    throw new IllegalArgumentException("Invalid start argument");
  }
  if (end < 0 || end > 23) {
    throw new IllegalArgumentException("Invalid end argument");
  }
  this.start=start;
  this.end=end;
  this.sequence=600;
  this.subSequence=start;
  if (Debug.verboseOn()) {
    Debug.logVerbose("Created " + this,module);
  }
}
