{
  double shipped=0.0;
  List<GenericValue> issues=this.getItemIssuances(orderId,orderItemSeqId,shipGroupSeqId);
  if (issues != null) {
    for (    GenericValue v : issues) {
      Double qty=v.getDouble("quantity");
      if (qty == null)       qty=Double.valueOf(0);
      shipped+=qty.doubleValue();
    }
  }
  return shipped;
}
