{
  if (this.active) {
    if (this.xid != null && this.xid.equals(xid)) {
      throw new XAException(XAException.XAER_DUPID);
    }
 else {
      throw new XAException(XAException.XAER_PROTO);
    }
  }
  if (this.xid != null && !this.xid.equals(xid)) {
    throw new XAException(XAException.XAER_NOTA);
  }
  this.xid=xid;
  this.active=true;
}
