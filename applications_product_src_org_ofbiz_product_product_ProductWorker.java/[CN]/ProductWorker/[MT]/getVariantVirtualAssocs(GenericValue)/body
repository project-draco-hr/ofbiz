{
  if (variantProduct != null && "Y".equals(variantProduct.getString("isVariant"))) {
    List productAssocs=EntityUtil.filterByDate(variantProduct.getRelatedByAndCache("AssocProductAssoc",UtilMisc.toMap("productAssocTypeId","PRODUCT_VARIANT")),true);
    return productAssocs;
  }
  return null;
}
