{
  Iterator i=null;
  if (memoryTable instanceof LRUMap) {
    i=((LRUMap)memoryTable).orderedMapIterator();
  }
 else {
    i=memoryTable.keySet().iterator();
  }
  int currentIdx=0;
  while (i.hasNext()) {
    Object key=i.next();
    if (currentIdx == index) {
      return key;
    }
  }
  return null;
}
