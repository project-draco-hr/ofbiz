{
  for (int i=0; i < questions.size(); i++) {
    ConfigItem ci=(ConfigItem)questions.get(i);
    if (!ci.isStandard()) {
      List options=ci.getOptions();
      for (int j=0; j < options.size(); j++) {
        ConfigOption co=(ConfigOption)options.get(j);
        co.setSelected(false);
        co.setComments(null);
      }
    }
  }
}
