{
  resetConfig();
  for (int i=0; i < questions.size(); i++) {
    ConfigItem ci=questions.get(i);
    if (ci.isMandatory()) {
      ConfigOption co=ci.getDefault();
      if (co != null) {
        co.setSelected(true);
      }
 else       if (ci.getOptions().size() > 0) {
        co=ci.getOptions().get(0);
        co.setSelected(true);
      }
    }
  }
}
