{
  Map storeFields=UtilMisc.toMap("productStoreId",productStoreId,"shipmentMethodTypeId",shipmentMethodTypeId,"partyId",carrierPartyId,"roleTypeId",carrierRoleTypeId);
  GenericValue storeShipMeth=null;
  try {
    storeShipMeth=delegator.findByPrimaryKeyCache("ProductStoreShipmentMeth",storeFields);
  }
 catch (  GenericEntityException e) {
    Debug.logError(e,module);
  }
  return storeShipMeth;
}
