{
  return "<set field=\"" + this.field + (this.valueExdr.isEmpty() ? "" : "\" value=\"" + this.valueExdr.getOriginal())+ (this.fromField.isEmpty() ? "" : "\" from-field=\"" + this.fromField)+ (this.defaultExdr.isEmpty() ? "" : "\" default-value=\"" + this.defaultExdr.getOriginal())+ (this.type == null || this.type.length() == 0 ? "" : "\" type=\"" + this.type)+ "\"/>";
}
