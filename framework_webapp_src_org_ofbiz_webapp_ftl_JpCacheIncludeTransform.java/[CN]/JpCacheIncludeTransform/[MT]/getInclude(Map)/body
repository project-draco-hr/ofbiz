{
  Object o=args.get("include");
  Debug.log("Include Object - " + o,module);
  String include=null;
  if (o != null) {
    if (o instanceof SimpleScalar) {
      SimpleScalar s=(SimpleScalar)o;
      include=s.getAsString();
      Debug.log("Include String - " + include,module);
    }
  }
  return include;
}
