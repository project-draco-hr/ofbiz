{
  Class<?>[] classArray=containerClass.getClasses();
  for (int i=0; i < classArray.length; i++) {
    try {
      if ((classArray[i].getModifiers() & Modifier.ABSTRACT) == 0) {
        classArray[i].newInstance();
      }
    }
 catch (    Exception e) {
      Debug.logError(e,module);
    }
  }
}
