{
  for (  Class<?> clz : containerClass.getClasses()) {
    try {
      if ((clz.getModifiers() & Modifier.ABSTRACT) == 0) {
        try {
          clz.getConstructor().newInstance();
        }
 catch (        NoSuchMethodException e) {
        }
      }
    }
 catch (    Exception e) {
      Debug.logError(e,module);
    }
  }
}
