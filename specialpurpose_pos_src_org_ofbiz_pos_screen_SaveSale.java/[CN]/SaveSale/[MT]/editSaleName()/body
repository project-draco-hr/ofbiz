{
  if (wasMouseClicked() && showKeyboardInSaveSale) {
    try {
      Keyboard keyboard=new Keyboard(m_pos);
      keyboard.setText(m_saleName.getText());
      m_saleName.setText(keyboard.openDlg());
    }
 catch (    Exception e) {
      Debug.logError(e,module);
    }
    m_dialog.repaint();
  }
  return;
}
