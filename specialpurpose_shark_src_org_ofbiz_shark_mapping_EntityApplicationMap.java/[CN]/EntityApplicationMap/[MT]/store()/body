{
  if (isNew) {
    try {
      delegator.create(application);
    }
 catch (    GenericEntityException e) {
      throw new RootException(e);
    }
  }
 else {
    try {
      delegator.store(application);
    }
 catch (    GenericEntityException e) {
      throw new RootException(e);
    }
  }
}
