{
  if (chars.length == 0) {
    return KEYBOARD_DATA;
  }
 else   if (((int)chars[0]) == 2 && ((int)chars[chars.length - 1]) == 10) {
    return SCANNER_DATA;
  }
 else   if (((int)chars[0]) == 37 && ((int)chars[chars.length - 1]) == 10) {
    return MSR_DATA;
  }
 else {
    return KEYBOARD_DATA;
  }
}
