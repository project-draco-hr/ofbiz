{
  if (shippableItemInfo != null) {
    Iterator<Map<String,Object>> i=shippableItemInfo.iterator();
    while (i.hasNext()) {
      Map<String,Object> testMap=i.next();
      String id=(String)testMap.get("productId");
      if (productId.equals(id)) {
        return testMap;
      }
    }
  }
  return null;
}
