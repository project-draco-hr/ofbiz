{
  Object value1=value.getValue(obj1);
  Object value2=value.getValue(obj2);
  int result;
  if (value1 == null) {
    result=value2 == null ? 0 : 1;
  }
 else   if (value2 == null) {
    result=value1 == null ? 0 : -1;
  }
 else {
    result=((Comparable)value1).compareTo(value2);
  }
  return descending ? -result : result;
}
