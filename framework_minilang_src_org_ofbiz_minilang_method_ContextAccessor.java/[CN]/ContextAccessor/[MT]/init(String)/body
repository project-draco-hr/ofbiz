{
  this.name=name;
  if (name == null || name.length() == 0) {
    empty=true;
    needsExpand=false;
    fma=new FlexibleMapAccessor(name);
  }
 else {
    empty=false;
    int openPos=name.indexOf("${");
    if (openPos != -1 && name.indexOf("}",openPos) != -1) {
      fma=null;
      needsExpand=true;
    }
 else {
      fma=new FlexibleMapAccessor(name);
      needsExpand=false;
    }
  }
}
