{
  if (description == null) {
    description="";
  }
  if (type == null) {
    type="";
  }
  if (title == null) {
    title="";
  }
  if (relEntityName == null) {
    relEntityName="";
  }
  if (fkName == null) {
    fkName="";
  }
  if (keyMaps == null) {
    keyMaps=Collections.EMPTY_LIST;
  }
 else {
    keyMaps=Collections.unmodifiableList(keyMaps);
  }
  return new ModelRelation(modelEntity,description,type,title,relEntityName,fkName,keyMaps,isAutoRelation);
}
