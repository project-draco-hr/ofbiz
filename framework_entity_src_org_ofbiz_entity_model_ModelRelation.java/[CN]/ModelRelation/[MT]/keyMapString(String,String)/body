{
  String returnString="";
  if (keyMaps.size() < 1) {
    return "";
  }
  int i=0;
  for (; i < keyMaps.size() - 1; i++) {
    returnString=returnString + ((ModelKeyMap)keyMaps.get(i)).fieldName + separator;
  }
  returnString=returnString + ((ModelKeyMap)keyMaps.get(i)).fieldName + afterLast;
  return returnString;
}
