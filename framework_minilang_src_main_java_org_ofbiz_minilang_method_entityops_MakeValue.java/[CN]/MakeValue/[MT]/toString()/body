{
  StringBuilder sb=new StringBuilder("<make-value ");
  sb.append("entity-name=\"").append(this.entityNameFse).append("\" ");
  sb.append("value-field=\"").append(this.valueFma).append("\" ");
  if (!mapFma.isEmpty()) {
    sb.append("map=\"").append(this.mapFma).append("\" ");
  }
  sb.append("/>");
  return sb.toString();
}
