{
  LocalDispatcher dispatcher=dctx.getDispatcher();
  GenericValue userLogin=(GenericValue)context.get("userLogin");
  String communicationEventId=(String)context.get("communicationEventId");
  try {
    Map<String,Object> result=dispatcher.runSync("updateCommunicationEvent",UtilMisc.<String,Object>toMap("communicationEventId",communicationEventId,"statusId","COM_COMPLETE","userLogin",userLogin));
    if (ServiceUtil.isError(result)) {
      return ServiceUtil.returnError(ServiceUtil.getErrorMessage(result));
    }
  }
 catch (  GenericServiceException esx) {
    return ServiceUtil.returnError(esx.getMessage());
  }
  return ServiceUtil.returnSuccess();
}
