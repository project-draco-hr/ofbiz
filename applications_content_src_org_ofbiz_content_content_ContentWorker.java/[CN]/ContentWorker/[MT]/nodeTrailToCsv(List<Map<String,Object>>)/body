{
  if (nodeTrail == null) {
    return "";
  }
  StringBuilder csv=new StringBuilder();
  Iterator<Map<String,Object>> it=nodeTrail.iterator();
  while (it.hasNext()) {
    if (csv.length() > 0) {
      csv.append(",");
    }
    Map<String,Object> node=it.next();
    if (node == null) {
      break;
    }
    String contentId=(String)node.get("contentId");
    csv.append(contentId);
  }
  return csv.toString();
}
