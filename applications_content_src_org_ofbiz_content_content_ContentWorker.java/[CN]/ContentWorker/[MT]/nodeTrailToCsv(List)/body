{
  if (nodeTrail == null)   return "";
  StringBuffer csv=new StringBuffer();
  Iterator it=nodeTrail.iterator();
  while (it.hasNext()) {
    if (csv.length() > 0)     csv.append(",");
    Map node=(Map)it.next();
    if (node == null)     break;
    String contentId=(String)node.get("contentId");
    csv.append(contentId);
  }
  return csv.toString();
}
