{
  Map<String,Collection<ServiceEcaRule>> eventMap=getServiceEventMap(serviceName);
  if (eventMap != null) {
    if (event != null) {
      return eventMap.get(event);
    }
 else {
      List<ServiceEcaRule> rules=FastList.newInstance();
      for (      Collection<ServiceEcaRule> col : eventMap.values()) {
        rules.addAll(col);
      }
      return rules;
    }
  }
  return null;
}
