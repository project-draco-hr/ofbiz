{
  if (lhs == null) {
    if (rhs != null) {
      return false;
    }
  }
 else   if (lhs instanceof String && rhs instanceof String) {
    try {
      return compilerMatcher.get().matches((String)lhs,makeOroPattern((String)rhs));
    }
 catch (    MalformedPatternException e) {
      Debug.logError(e,module);
      return false;
    }
  }
  return true;
}
