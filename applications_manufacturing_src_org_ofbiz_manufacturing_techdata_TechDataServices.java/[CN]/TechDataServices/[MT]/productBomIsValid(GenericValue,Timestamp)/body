{
  if (productBom.getTimestamp("fromDate") != null)   if (productBom.getTimestamp("thruDate") != null)   if (testDate.after(productBom.getTimestamp("fromDate")) && testDate.before(productBom.getTimestamp("thruDate")))   return true;
 else   return false;
 else   if (testDate.after(productBom.getTimestamp("fromDate")))   return true;
 else   return false;
 else   if (productBom.getTimestamp("thruDate") != null)   if (testDate.before(productBom.getTimestamp("thruDate")))   return true;
 else   return false;
 else   return true;
}
