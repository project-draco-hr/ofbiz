{
  if (values == null)   return null;
  if (UtilValidate.isEmpty(exprs)) {
    return values;
  }
  List<T> result=new LinkedList<T>();
  for (  T value : values) {
    boolean include=true;
    for (    EntityCondition condition : exprs) {
      include=condition.entityMatches(value);
      if (!include)       break;
    }
    if (include) {
      result.add(value);
    }
  }
  return result;
}
