{
  if (expression == null || expression.length() == 0) {
    Debug.logVerbose("Null or empty expression, returning true.",module);
    return true;
  }
  Object o=null;
  try {
    o=BshUtil.eval(expression.trim(),context);
  }
 catch (  bsh.EvalError e) {
    throw new WfException("Bsh evaluation error.",e);
  }
  if (o == null)   return false;
 else   if (o instanceof Number)   return (((Number)o).doubleValue() == 0) ? false : true;
 else   return (!o.toString().equalsIgnoreCase("true")) ? false : true;
}
