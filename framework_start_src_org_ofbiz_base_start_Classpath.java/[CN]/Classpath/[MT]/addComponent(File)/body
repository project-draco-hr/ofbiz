{
  if (component != null) {
    try {
      if (component.exists()) {
        File key=component.getCanonicalFile();
        if (!_elements.contains(key)) {
          _elements.add(key);
          return true;
        }
      }
 else {
        System.out.println("Warning : Module classpath component '" + component + "' is not valid and will be ignored...");
      }
    }
 catch (    IOException e) {
    }
  }
  return false;
}
