{
  try {
    tomcat.getServer().start();
  }
 catch (  LifecycleException e) {
    throw new ContainerException(e);
  }
  loadComponents();
  for (  Connector con : tomcat.getService().findConnectors()) {
    ProtocolHandler ph=con.getProtocolHandler();
    if (ph instanceof Http11Protocol) {
      Http11Protocol hph=(Http11Protocol)ph;
      Debug.logInfo("Connector " + hph.getName() + " @ "+ hph.getPort()+ " - "+ (hph.getSecure() ? "secure" : "not-secure")+ " ["+ con.getProtocolHandlerClassName()+ "] started.",module);
    }
 else {
      Debug.logInfo("Connector " + con.getProtocol() + " @ "+ con.getPort()+ " - "+ (con.getSecure() ? "secure" : "not-secure")+ " ["+ con.getProtocolHandlerClassName()+ "] started.",module);
    }
  }
  Debug.logInfo("Started " + ServerInfo.getServerInfo(),module);
  return true;
}
