{
  long stagger=1;
  if (staggerMax > 1) {
    stagger=(long)Math.ceil(Math.random() * staggerMax);
    if (stagger == 0)     stagger=1;
  }
synchronized (this) {
    if ((curSeqId + stagger) <= maxSeqId) {
      Long retSeqId=Long.valueOf(curSeqId);
      curSeqId+=stagger;
      return retSeqId;
    }
 else {
      fillBank(stagger);
      if ((curSeqId + stagger) <= maxSeqId) {
        Long retSeqId=Long.valueOf(curSeqId);
        curSeqId+=stagger;
        return retSeqId;
      }
 else {
        Debug.logError("Fill bank failed, returning null",module);
        return null;
      }
    }
  }
}
