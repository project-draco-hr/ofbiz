{
  StringBuilder sb=new StringBuilder("<now ");
  if (!this.fieldFma.isEmpty()) {
    sb.append("field=\"").append(this.fieldFma).append("\" ");
  }
  if (!this.resultFma.isEmpty()) {
    sb.append("result-name=\"").append(this.resultFma).append("\" ");
  }
  sb.append("/>");
  return sb.toString();
}
