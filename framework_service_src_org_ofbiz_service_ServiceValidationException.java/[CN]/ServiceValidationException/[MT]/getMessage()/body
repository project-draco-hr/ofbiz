{
  String msg=super.getMessage();
  if (this.messages != null && this.messages.size() > 0) {
    StringBuilder sb=new StringBuilder();
    if (msg != null) {
      sb.append(msg).append('\n');
    }
    Iterator i=this.messages.iterator();
    while (i.hasNext()) {
      sb.append(i.next());
    }
    msg=sb.toString();
  }
  return msg;
}
