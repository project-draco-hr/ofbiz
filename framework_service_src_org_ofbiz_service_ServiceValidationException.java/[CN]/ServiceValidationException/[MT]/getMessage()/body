{
  String msg=super.getMessage();
  if (this.messages != null && this.messages.size() > 0) {
    StringBuilder sb=new StringBuilder();
    if (msg != null) {
      sb.append(msg).append('\n');
    }
    for (    String m : this.messages) {
      sb.append(m);
    }
    msg=sb.toString();
  }
  return msg;
}
