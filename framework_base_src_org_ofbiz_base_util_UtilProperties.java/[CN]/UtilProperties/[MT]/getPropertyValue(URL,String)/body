{
  if (url == null)   return "";
  if (name == null || name.length() <= 0)   return "";
  Properties properties=getProperties(url);
  if (properties == null) {
    return null;
  }
  String value=null;
  try {
    value=properties.getProperty(name);
  }
 catch (  Exception e) {
    Debug.log(e.getMessage(),module);
  }
  return value == null ? "" : value.trim();
}
