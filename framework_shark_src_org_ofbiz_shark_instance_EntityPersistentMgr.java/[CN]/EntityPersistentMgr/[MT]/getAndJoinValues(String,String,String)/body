{
  GenericDelegator delegator=SharkContainer.getDelegator();
  List lookupList=null;
  try {
    lookupList=delegator.findByAnd("WfAndJoin",UtilMisc.toMap("processId",processId,"activitySetDefId",activitySetDefId,"activityDefId",activityDefId));
  }
 catch (  GenericEntityException e) {
    throw new PersistenceException(e);
  }
  if (lookupList == null) {
    lookupList=new ArrayList();
  }
  return lookupList;
}
