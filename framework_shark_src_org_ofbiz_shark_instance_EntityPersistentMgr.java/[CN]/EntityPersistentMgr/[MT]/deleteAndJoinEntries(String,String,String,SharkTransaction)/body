    delegator.removeByAnd(org.ofbiz.shark.SharkConstants.WfAndJoin,UtilMisc.toMap(org.ofbiz.shark.SharkConstants.processId,procId,org.ofbiz.shark.SharkConstants.activitySetDefId,asDefId,org.ofbiz.shark.SharkConstants.activityDefId,aDefId));
  }
 catch (  GenericEntityException e) {
