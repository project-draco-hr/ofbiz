{
  GenericValue dataObject=getRuntimeObject();
  String contextXML=null;
  Map<String,Object> context=null;
  if (dataObject.get("runtimeDataId") == null)   return context;
  try {
    GenericValue runtimeData=dataObject.getRelatedOne("RuntimeData",false);
    contextXML=runtimeData.getString("runtimeInfo");
  }
 catch (  GenericEntityException e) {
    throw new WfException(e.getMessage(),e);
  }
  if (contextXML != null) {
    try {
      context=UtilGenerics.checkMap(XmlSerializer.deserialize(contextXML,getDelegator()));
    }
 catch (    SerializeException e) {
      throw new WfException(e.getMessage(),e);
    }
catch (    IOException e) {
      throw new WfException(e.getMessage(),e);
    }
catch (    Exception e) {
      throw new WfException(e.getMessage(),e);
    }
  }
  return context;
}
