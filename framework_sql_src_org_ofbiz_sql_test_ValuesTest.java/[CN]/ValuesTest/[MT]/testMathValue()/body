  mathValueTest("v3",v3,"+",list(nv1,nv2),"(1 + 2.0)",v1,true);
  MathValue v4=new MathValue("-",GenericTestCaseBase.<ConstantValue>list(nv1,nv3));
  mathValueTest("v4",v4,"-",list(nv1,nv3),"(1 - 3)",v1,false);
