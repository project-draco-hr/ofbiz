{
  for (  String loaderClassName : config.loaders) {
    try {
      Class loaderClass=classloader.loadClass(loaderClassName);
      StartupLoader loader=(StartupLoader)loaderClass.newInstance();
      loader.load(config,loaderArgs);
      loaders.add(loader);
    }
 catch (    Exception e) {
      e.printStackTrace();
      System.exit(99);
    }
  }
}
