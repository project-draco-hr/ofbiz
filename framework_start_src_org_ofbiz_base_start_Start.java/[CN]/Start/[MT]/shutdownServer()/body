{
  if (serverStopping)   return;
  serverStopping=true;
synchronized (this.loaders) {
    for (int i=this.loaders.size(); i > 0; i--) {
      StartupLoader loader=this.loaders.get(i - 1);
      try {
        loader.unload();
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
  serverRunning=false;
}
