{
synchronized (this.loaders) {
    for (    StartupLoader loader : this.loaders) {
      if (this.serverStopping) {
        return false;
      }
      try {
        loader.start();
      }
 catch (      StartupException e) {
        e.printStackTrace();
        if (DEBUG == null)         return false;
      }
catch (      Error e) {
        e.printStackTrace();
        if (DEBUG == null)         return false;
      }
    }
    serverStarted=true;
  }
  return true;
}
