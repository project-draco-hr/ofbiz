{
synchronized (this.loaders) {
    for (    StartupLoader loader : this.loaders) {
      if (this.serverState.get() == ServerState.STOPPING) {
        return false;
      }
      try {
        loader.start();
      }
 catch (      StartupException e) {
        e.printStackTrace();
        return false;
      }
    }
    this.serverState.set(ServerState.RUNNING);
  }
  return true;
}
