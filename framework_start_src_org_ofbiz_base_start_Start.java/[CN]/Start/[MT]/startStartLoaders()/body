{
synchronized (this.loaders) {
    for (    StartupLoader loader : this.loaders) {
      if (this.serverStopping) {
        return;
      }
      try {
        loader.start();
      }
 catch (      StartupException e) {
        e.printStackTrace();
        System.exit(99);
      }
    }
    serverStarted=true;
  }
}
