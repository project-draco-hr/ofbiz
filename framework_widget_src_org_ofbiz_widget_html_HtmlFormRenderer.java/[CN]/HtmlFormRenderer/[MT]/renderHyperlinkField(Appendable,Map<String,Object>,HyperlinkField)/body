{
  this.request.setAttribute("image",hyperlinkField.getImage());
  ModelFormField modelFormField=hyperlinkField.getModelFormField();
  this.makeHyperlinkString(writer,modelFormField.getWidgetStyle(),hyperlinkField.getTargetType(),hyperlinkField.getTarget(context),hyperlinkField.getDescription(context),hyperlinkField.getTargetWindow(context),modelFormField.getEvent(),modelFormField.getAction(context));
  this.appendTooltip(writer,context,modelFormField);
}
