{
  String result=target;
  String externalLoginKey=(String)this.request.getAttribute("externalLoginKey");
  if (UtilValidate.isNotEmpty(externalLoginKey)) {
    if (target.contains("?")) {
      result=target + "&externalLoginKey=" + externalLoginKey;
    }
 else {
      result=target + "?externalLoginKey=" + externalLoginKey;
    }
  }
  return result;
}
