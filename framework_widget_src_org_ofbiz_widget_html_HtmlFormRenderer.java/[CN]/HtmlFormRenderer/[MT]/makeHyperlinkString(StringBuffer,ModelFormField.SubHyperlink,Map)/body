{
  if (subHyperlink == null) {
    return;
  }
  if (subHyperlink.shouldUse(context)) {
    buffer.append(' ');
    this.makeHyperlinkString(buffer,subHyperlink.getLinkStyle(),subHyperlink.getTargetType(),subHyperlink.getTarget(context),subHyperlink.getDescription(context),subHyperlink.getTargetWindow(context),null,null);
  }
}
