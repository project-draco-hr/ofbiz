{
  String titleStyle=modelFormField.getTitleStyle();
  if (UtilValidate.isNotEmpty(titleStyle)) {
    writer.write("<span class=\"");
    writer.write(titleStyle);
    writer.write("\">");
  }
  if (isLast) {
    writer.write(" - ");
  }
 else {
    writer.write(" - ");
  }
  if (UtilValidate.isNotEmpty(titleStyle)) {
    writer.write("</span>");
  }
}
