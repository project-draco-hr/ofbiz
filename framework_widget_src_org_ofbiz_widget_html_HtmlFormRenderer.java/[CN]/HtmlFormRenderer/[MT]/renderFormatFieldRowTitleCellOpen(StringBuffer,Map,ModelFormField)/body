{
  buffer.append("<td width=\"20%\" align=\"right\"");
  String areaStyle=modelFormField.getTitleAreaStyle();
  if (UtilValidate.isNotEmpty(areaStyle)) {
    buffer.append(" class=\"");
    buffer.append(areaStyle);
    buffer.append("\"");
  }
  buffer.append(">");
  this.appendWhitespace(buffer);
}
