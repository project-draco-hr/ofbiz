{
  writer.write("<input type=\"hidden\"");
  writer.write(" name=\"");
  writer.write(modelFormField.getParameterName(context));
  writer.write('"');
  if (UtilValidate.isNotEmpty(value)) {
    writer.write(" value=\"");
    writer.write(StringEscapeUtils.escapeHtml(value));
    writer.write('"');
  }
  writer.write("/>");
  appendWhitespace(writer);
}
