{
  buffer.append("<span");
  String titleStyle=modelFormField.getTitleStyle();
  if (UtilValidate.isNotEmpty(titleStyle)) {
    buffer.append(" class=\"");
    buffer.append(titleStyle);
    buffer.append("\"");
  }
  buffer.append(">");
  if (isLast) {
    buffer.append(" - ");
  }
 else {
    buffer.append(" - ");
  }
  buffer.append("</span>");
}
