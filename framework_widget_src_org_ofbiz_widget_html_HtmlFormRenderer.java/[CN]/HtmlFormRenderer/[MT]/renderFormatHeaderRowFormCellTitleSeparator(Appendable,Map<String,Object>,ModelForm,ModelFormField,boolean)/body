{
  String titleStyle=modelFormField.getTitleStyle();
  if (UtilValidate.isNotEmpty(titleStyle)) {
    writer.append("<span class=\"");
    writer.append(titleStyle);
    writer.append("\">");
  }
  if (isLast) {
    writer.append(" - ");
  }
 else {
    writer.append(" - ");
  }
  if (UtilValidate.isNotEmpty(titleStyle)) {
    writer.append("</span>");
  }
}
