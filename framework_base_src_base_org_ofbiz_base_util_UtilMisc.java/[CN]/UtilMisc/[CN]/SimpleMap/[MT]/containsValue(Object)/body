{
  if (realMapIfNeeded != null) {
    return realMapIfNeeded.containsValue(obj);
  }
 else {
    for (int i=0; i < names.length; i++) {
      if (obj == null && values[i] == null)       return true;
      if (values[i] != null && values[i].equals(obj))       return true;
    }
    return false;
  }
}
