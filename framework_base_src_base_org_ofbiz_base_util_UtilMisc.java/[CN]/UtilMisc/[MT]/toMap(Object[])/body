{
  if (data == null) {
    return null;
  }
  if (data.length % 2 == 1) {
    throw new IllegalArgumentException("You must pass an even sized array to the toMap method");
  }
  Map map=FastMap.newInstance();
  for (int i=0; i < data.length; ) {
    map.put(data[i++],data[i++]);
  }
  return map;
}
