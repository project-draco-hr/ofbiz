{
  if (session != null && session.isLive()) {
    session.logout();
  }
  if (removeRepositoryOnShutdown) {
    if (UtilValidate.isNotEmpty(homeDir)) {
      File homeDirFile=new File(homeDir);
      try {
        FileUtils.deleteDirectory(homeDirFile);
      }
 catch (      IOException e) {
        Debug.logError(e,module);
      }
    }
  }
}
