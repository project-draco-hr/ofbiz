{
  for (  PackingSessionLine line : this.getLines()) {
    if (orderId.equals(line.getOrderId()) && orderItemSeqId.equals(line.getOrderItemSeqId()) && shipGroupSeqId.equals(line.getShipGroupSeqId())&& productId.equals(line.getProductId())&& inventoryItemId.equals(line.getInventoryItemId())&& packageSeq == line.getPackageSeq()) {
      return line;
    }
  }
  return null;
}
