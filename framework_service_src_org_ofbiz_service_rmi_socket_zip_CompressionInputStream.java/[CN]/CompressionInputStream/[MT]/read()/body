{
  try {
    int code;
    do {
      code=readCode();
    }
 while (code == NOP);
    if (code >= BASE) {
      return codeTable.charAt(code - BASE);
    }
 else     if (code == RAW) {
      int high=readCode();
      int low=readCode();
      return (high << 4) | low;
    }
 else     throw new IOException("unknown compression code: " + code);
  }
 catch (  EOFException e) {
    return -1;
  }
}
