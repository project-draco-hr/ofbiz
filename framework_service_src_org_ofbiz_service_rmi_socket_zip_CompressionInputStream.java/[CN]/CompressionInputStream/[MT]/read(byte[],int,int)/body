{
  if (len <= 0) {
    return 0;
  }
  int c=read();
  if (c == -1) {
    return -1;
  }
  b[off]=(byte)c;
  int i=1;
  try {
    for (; (i < len) && (in.available() > 0); i++) {
      c=read();
      if (c == -1) {
        break;
      }
      if (b != null) {
        b[off + i]=(byte)c;
      }
    }
  }
 catch (  IOException ee) {
  }
  return i;
}
