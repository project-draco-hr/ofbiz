{
  if (bufPos == 5) {
    int b1=in.read();
    int b2=in.read();
    int b3=in.read();
    int b4=in.read();
    if ((b1 | b2 | b3| b4) < 0) {
      throw new EOFException();
    }
    int pack=(b1 << 24) | (b2 << 16) | (b3 << 8)| b4;
    buf[0]=(pack >>> 24) & 0x3F;
    buf[1]=(pack >>> 18) & 0x3F;
    buf[2]=(pack >>> 12) & 0x3F;
    buf[3]=(pack >>> 6) & 0x3F;
    buf[4]=(pack >>> 0) & 0x3F;
    bufPos=0;
  }
  return buf[bufPos++];
}
