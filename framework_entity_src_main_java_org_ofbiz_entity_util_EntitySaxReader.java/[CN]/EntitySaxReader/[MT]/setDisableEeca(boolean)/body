{
  this.disableEeca=disableEeca;
  if (disableEeca) {
    if (this.ecaHandler == null) {
      this.ecaHandler=delegator.getEntityEcaHandler();
    }
    this.delegator.setEntityEcaHandler(null);
  }
 else {
    if (ecaHandler != null) {
      this.delegator.setEntityEcaHandler(ecaHandler);
    }
  }
}
