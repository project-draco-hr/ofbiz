{
  if (bytes != null) {
    _ps.setBytes(_ind,bytes);
  }
 else {
    DatasourceInfo datasourceInfo=EntityConfigUtil.getDatasourceInfo(this.helperName);
    if (datasourceInfo.useBinaryTypeForBlob) {
      _ps.setNull(_ind,Types.BINARY);
    }
 else {
      _ps.setNull(_ind,Types.BLOB);
    }
  }
  _ind++;
}
