{
  if (message == null) {
    if (serializedBytes != null) {
      ByteArrayInputStream bais=new ByteArrayInputStream(serializedBytes);
      try {
        message=new MimeMessage(this.getSession(),bais);
      }
 catch (      MessagingException e) {
        Debug.logError(e,module);
      }
    }
  }
  return message;
}
