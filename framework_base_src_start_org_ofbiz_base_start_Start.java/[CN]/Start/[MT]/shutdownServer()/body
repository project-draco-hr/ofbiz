{
  if (serverStopping)   return;
  serverStopping=true;
  if (loaders != null && loaders.size() > 0) {
    Iterator i=loaders.iterator();
    while (i.hasNext()) {
      StartupLoader loader=(StartupLoader)i.next();
      try {
        loader.unload();
      }
 catch (      Exception e) {
        e.printStackTrace();
      }
    }
  }
  serverRunning=false;
}
