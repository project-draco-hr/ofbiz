{
  sb.append("RELATION");
  if (type != null) {
    sb.append(" TYPE ").append(type);
  }
  if (title != null) {
    sb.append(" TITLE ").append(title);
  }
  sb.append(' ').append(entityName);
  sb.append(" ON");
  for (int i=0; i < keyMaps.size(); i++) {
    sb.append(' ');
    keyMaps.get(i).appendTo("cur","other",sb);
  }
  return sb;
}
