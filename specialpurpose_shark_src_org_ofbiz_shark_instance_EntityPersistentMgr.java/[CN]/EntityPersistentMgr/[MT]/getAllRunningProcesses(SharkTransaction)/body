{
  Delegator delegator=SharkContainer.getDelegator();
  List runningStates=UtilMisc.toList("open.running");
  List order=UtilMisc.toList(org.ofbiz.shark.SharkConstants.startedTime);
  List createdList=new ArrayList();
  List lookupList=null;
  try {
    lookupList=delegator.findList(org.ofbiz.shark.SharkConstants.WfProcess,makeStateListCondition(org.ofbiz.shark.SharkConstants.currentState,runningStates,EntityOperator.EQUALS,EntityOperator.OR),null,order,null,false);
  }
 catch (  GenericEntityException e) {
    throw new PersistenceException(e);
  }
  if (!UtilValidate.isEmpty(lookupList)) {
    Iterator i=lookupList.iterator();
    while (i.hasNext()) {
      GenericValue v=(GenericValue)i.next();
      createdList.add(Process.getInstance(this,v));
    }
  }
  return createdList;
}
