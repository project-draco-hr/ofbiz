{
  GenericDelegator delegator=SharkContainer.getDelegator();
  List lookupList=null;
  try {
    lookupList=delegator.findByAnd(org.ofbiz.shark.SharkConstants.WfAndJoin,UtilMisc.toMap(org.ofbiz.shark.SharkConstants.processId,processId,org.ofbiz.shark.SharkConstants.activitySetDefId,activitySetDefId,org.ofbiz.shark.SharkConstants.activityDefId,activityDefId));
  }
 catch (  GenericEntityException e) {
    throw new PersistenceException(e);
  }
  if (lookupList == null) {
    lookupList=new ArrayList();
  }
  return lookupList;
}
