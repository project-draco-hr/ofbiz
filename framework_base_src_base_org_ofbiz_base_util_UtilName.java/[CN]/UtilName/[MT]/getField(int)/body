{
  int index[]=this.getFieldIndex(field);
  if (index != null && index.length > 0) {
    if (index.length == 1) {
      return name[index[0]];
    }
 else {
      StringBuilder nameBuf=new StringBuilder();
      for (int i=0; i < index.length; i++) {
        if (nameBuf.length() > 0) {
          nameBuf.append(" ");
        }
        nameBuf.append(name[index[i]]);
      }
      return nameBuf.toString();
    }
  }
  return null;
}
