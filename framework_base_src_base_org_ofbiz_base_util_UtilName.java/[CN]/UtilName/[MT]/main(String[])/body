{
  StringBuilder name=new StringBuilder();
  for (int i=0; i < args.length; i++) {
    if (name.length() != 0) {
      name.append(" ");
    }
    name.append(args[i]);
  }
  Map nameMap=parseName(name.toString(),true);
  Iterator i=nameMap.keySet().iterator();
  while (i.hasNext()) {
    String f=(String)i.next();
    System.out.println(f + " - " + nameMap.get(f));
  }
}
