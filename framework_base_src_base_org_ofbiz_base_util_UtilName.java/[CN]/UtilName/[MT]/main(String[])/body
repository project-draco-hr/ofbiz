{
  String name="";
  for (int i=0; i < args.length; i++) {
    if (!"".equals(name)) {
      name=name + " ";
    }
    name=name + args[i];
  }
  Map nameMap=parseName(name,true);
  Iterator i=nameMap.keySet().iterator();
  while (i.hasNext()) {
    String f=(String)i.next();
    System.out.println(f + " - " + nameMap.get(f));
  }
}
